    //
    // Individuals
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://w3id.org/radx/DATAHUB_0001002 -->

    <NamedIndividual rdf:about="http://w3id.org/radx/DATAHUB_0001002">
        <rdf:type rdf:resource="http://w3id.org/radx/DATAHUB_0001001"/>
        <rdfs:label>radx radical (radx-rad)</rdfs:label>
        <skos:altLabel>RADx-rad
non-traditional diagnostics</skos:altLabel>
        <skos:definition>A RADx effort focusing on innovative, non-traditional COVID-19 testing approaches, including rapid detection devices and home-based testing technologies. RADx-rad seeks to address gaps in usability, accessibility, and accuracy.</skos:definition>
    </NamedIndividual>
    


    <!-- http://w3id.org/radx/DATAHUB_0001003 -->

    <NamedIndividual rdf:about="http://w3id.org/radx/DATAHUB_0001003">
        <rdf:type rdf:resource="http://w3id.org/radx/DATAHUB_0001001"/>
        <rdfs:label>radx tech</rdfs:label>
        <skos:altLabel>point-of-care technology
rapid diagnostics</skos:altLabel>
        <skos:definition>RADx Tech (Rapid Acceleration of Diagnostics Technology) is a component program of the NIH RADx initiative that accelerated the development, validation, and commercialization of innovative point-of-care and home-based diagnostic technologies for COVID-19 detection, as well as improvements to clinical laboratory testing methods that directly detect the virus.</skos:definition>
    </NamedIndividual>
    


    <!-- http://w3id.org/radx/DATAHUB_0001004 -->

    <NamedIndividual rdf:about="http://w3id.org/radx/DATAHUB_0001004">
        <rdf:type rdf:resource="http://w3id.org/radx/DATAHUB_0001001"/>
        <rdfs:label>radx underserved populations (radx-up)</rdfs:label>
        <skos:altLabel>RADx-UP
underserved research</skos:altLabel>
        <skos:definition>An initiative aimed at reducing health disparities in COVID-19 morbidity and mortality among underserved and vulnerable populations disproportionately affected by the pandemic. The goal is to understand and address the factors driving these disparities.</skos:definition>
    </NamedIndividual>
    


    <!-- http://w3id.org/radx/DATAHUB_0002002 -->

    <NamedIndividual rdf:about="http://w3id.org/radx/DATAHUB_0002002">
        <rdfs:label>RADx</rdfs:label>
        <skos:altLabel>Rapid Acceleration of Diagnostics
RADx Program</skos:altLabel>
        <skos:definition>RADx stands for Rapid Acceleration of Diagnostics. It’s a program launched by the National Institutes of Health (NIH) in 2020, as part of the U.S. response to the COVID-19 pandemic. The goal of RADx was to accelerate the development, commercialization, and deployment of innovative diagnostic technologies—especially for COVID-19 testing.</skos:definition>
    </NamedIndividual>
    


    <!-- http://w3id.org/radx/DATAHUB_0002007 -->

    <NamedIndividual rdf:about="http://w3id.org/radx/DATAHUB_0002007">
        <rdf:type rdf:resource="http://w3id.org/radx/DATAHUB_0002006"/>
        <rdfs:label>bioportal ontology repository</rdfs:label>
        <skos:altLabel>ontology repository
bioportal</skos:altLabel>
        <skos:definition>BioPortal is an ontology repository developed and maintained by the National Center for Biomedical Ontology (NCBO). It provides a web-based platform for accessing, sharing, visualizing, and analyzing biomedical ontologies.</skos:definition>
    </NamedIndividual>
    


    <!-- http://w3id.org/radx/DATAHUB_0002008 -->

    <NamedIndividual rdf:about="http://w3id.org/radx/DATAHUB_0002008">
        <schema:example>Booz Allen Hamilton developed and maintains the RADx Data Hub, supporting data submission, curation, and access processes for all participating RADx studies.</schema:example>
        <rdfs:label>Booz Allen Hamilton</rdfs:label>
        <skos:altLabel>Booz Allen
Booz partner</skos:altLabel>
        <skos:definition>Booz Allen Hamilton is a management consulting and technology consulting firm, with a strong focus on serving the U.S. government, particularly in areas like defense, intelligence, and civil agencies. Founded in 1914, Booz Allen is known for providing strategy, analytics, digital solutions, engineering, and cybersecurity services to clients in both the public and private sectors.</skos:definition>
    </NamedIndividual>
    


    <!-- http://w3id.org/radx/DATAHUB_0002009 -->

    <NamedIndividual rdf:about="http://w3id.org/radx/DATAHUB_0002009">
        <rdfs:label>center for expanded data annotation and retrieval</rdfs:label>
        <skos:altLabel>CEDAR
data annotation center</skos:altLabel>
        <skos:definition>A web-based computational metadata management platform focused on helping users create high-quality biomedical metadata through templates that facilitate development, evaluation, use, and refinement of experiment-based biomedical metadata.</skos:definition>
    </NamedIndividual>
    


